{
  "entryFile": "src/server.ts",
  "noImplicitAdditionalProperties": "silently-remove",
  "controllerPathGlobs": ["src/controllers/*.ts"],
  "spec": {
    "outputDirectory": "dist",
    "specVersion": 3,
    "security": {
      "jwt": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  },
  "routes": {
    "routesDir": "src/routes",
    "authenticationModule": "./src/middleware/expressAuthentication.ts"
}
}
